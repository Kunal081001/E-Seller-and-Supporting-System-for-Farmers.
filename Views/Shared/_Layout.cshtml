<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>

    <nav>
        <div class="menu">
            <div class="logo">
                @Html.ActionLink("E-Seller System", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })

            </div>
            <ul>
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li>@Html.ActionLink("Product", "Product", "Home")</li>
                <li>@Html.ActionLink("Farmer Login", "FLogin", "Home")</li>
                <li>@Html.ActionLink("About Us", "About", "Home")</li>
                <li>@Html.ActionLink("Contact Us", "Contact", "Home")</li>
            </ul>
        </div>
    </nav>

    <div class="container body-content">
        <div class="container">
            @RenderBody()
        </div>
        <br />
        <br />
        <hr />

    </div>
    <footer>
        <div class="content">
            <div class="top">
                <div class="logo-details">
                    <i class="fab fa-slack"></i>
                    <span class="logo_name">Mind Install</span>
                </div>

            </div>
            <div class="link-boxes">
                <ul class="box">
                    <li class="link_name">Company</li>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
                <ul class="box">
                    <li class="link_name">Account</li>
                    <li><a href="#">Farmer</a></li>
                    <li><a href="#">User</a></li>

                </ul>
                <ul class="box">
                    <li class="link_name">Connect with us</li>
                    <li><a href="#">Address : SSBT COET Jalgaon</a> </li>
                    <li><a href="#">Email : mindinstall2022@gmail.com </a></li>
                    <li><a href="#">Phone : 95453xxxxx </a></li>
                </ul>
                @*<ul class="box input-box">
                        <li class="link_name">Feedback</li>
                        <li>
                            <textarea name="feedback" id="feed" cols="20" rows="1" placeholder="Enter Your Feedback Here"></textarea>
                        </li>
                        <li><input type="button" value="Submit"></li>
                    </ul>*@
            </div>
        </div>
        <div class="bottom-details">
            <div class="bottom_text">
                <span class="copyright_text">Copyright © 2022 <a href="#">mindInstall.</a>All rights reserved</span>
                <span class="policy_terms">
                    <a href="#">Privacy policy</a>
                    <a href="#">Terms & condition</a>
                </span>
            </div>
        </div>
    </footer>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>



























